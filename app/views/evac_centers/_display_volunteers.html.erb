<table class="table table-striped table-hover border rounded">
  <thead>
    <tr>
      <th >First Name</th>
      <th >Last Name</th>
      <th >Contact Number</th>
      <th >Actions</th>
    </tr>
  </thead>
  <tbody class="table-group-divider">
    <% assigned_yearly_vol.each do |av| %>
      <% user = User.find(av.volunteer_id) %>
      <tr class="">
        <td><%= user.fname%></td>
        <td><%= user.lname%></td>
        <td><%= user.cnum%></td>
        <td class="position-relative">
          <div class = "row">
            <div class = "col-md-6">
              <%= render partial: "/layouts/delete_button", locals: {link: "/remove_volunteer/#{av.id}"}%>
            </div>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
